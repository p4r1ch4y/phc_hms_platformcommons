{
    "name": "@phc/database",
    "version": "1.0.0",
    "main": "index.ts",
    "scripts": {
        "build": "npx prisma generate --schema=./prisma/schema.prisma && npx prisma generate --schema=./prisma/tenant.prisma && npx prisma generate --schema=./prisma/pharmacy.prisma",
        "generate": "prisma generate --schema=./prisma/schema.prisma && prisma generate --schema=./prisma/tenant.prisma",
        "migrate:management": "prisma migrate dev --schema=./prisma/schema.prisma --name init_management",
        "push:tenant": "prisma db push --schema=./prisma/tenant.prisma",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "dependencies": {
        "@prisma/client": "^5.10.2"
    },
    "devDependencies": {
        "prisma": "^5.10.2",
        "typescript": "^5.3.3",
        "@types/node": "^20.11.24"
    }
}